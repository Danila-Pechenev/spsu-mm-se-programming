            <RowDefinition Height="21*"/>
            <RowDefinition Height="21*"/>
            <RowDefinition Height="21*"/>
            <RowDefinition Height="232*"/>
        </Grid.RowDefinitions>
        <Border Background="#FF2BBEE2" Grid.ColumnSpan="8"/>
        <Border Background="#FF111B1D" Margin="0,46,0,0" Grid.ColumnSpan="8"/>
        <Border Background="#FF111B1D" Margin="0,0,155,6" Grid.RowSpan="5" Grid.Column="1"/>
        <Label Grid.Row="0" Grid.Column="0" Content="WebService" FontSize="20"  FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Grid.Row="0" Grid.Column="1" Content="Temperature(C)" FontSize="20"  FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Grid.Row="0" Grid.Column="2" Content="Temperature(F)" FontSize="20"  FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Grid.Row="0" Grid.Column="3" Content="Cloud Cover(%)" FontSize="20"  FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Grid.Row="0" Grid.Column="4" Content="Humidity(%)" FontSize="20"  FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Grid.Row="0" Grid.Column="5" Content="Precipipation(type,mm)" FontSize="20"  FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Grid.Row="0" Grid.Column="6" Content="WindSpeed(m/s)" FontSize="20"  FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Grid.Row="0" Grid.Column="7" Content="WindDegree" FontSize="20"  FontFamily="Ink Free" FontWeight="Bold"/>

